{
    "name": "ziiven/flarum-tag-background",
    "description": "A Flarum extension. ",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0"
    },
    "support": {
        "issues": "https://github.com/Ziiven/flarum-tag-background/issues",
        "source": "https://github.com/Ziiven/flarum-tag-background"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ziven\\tagBackground\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ziven"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "Ziven Tag Background",
            "icon": {
                "name": "fas fa-images",
                "backgroundColor": "#68238c",
                "color": "#fff"
            }
        }
    }
}